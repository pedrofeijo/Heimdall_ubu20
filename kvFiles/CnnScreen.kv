<CnnScreen>
        canvas.before:
                Rectangle:
                        pos: self.pos
                        size: self.size
                        source: 'kvFiles/appImages/backgroundImages/heimdall.jpg'  # 'Background.jpeg'

        BoxLayout:
                BoxLayout:
                        orientation: "vertical"
                        size_hint_y: .2
                        pos_hint : {'y':.75}

                        Button:
                                text: 'Select Files'
                        Button:
                                text: 'Menu'
                                on_release: root.manager.current = 'mainScreenName'
