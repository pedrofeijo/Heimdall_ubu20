<CsvConverterYoloScreen>
        center_x: root.width/2
        center_y: root.height/2

        orientation: "vertical"

        canvas.before:


                Color:
                        rgba: 1, 1, 1, 0.4

                Rectangle:

                        pos: self.pos
                        size: self.size
                        source: 'kvFiles/appImages/backgroundImages/bifrost.png'  # 'Background.jpeg'

        BoxLayout:

                BoxLayout:
                        id : boxLayoutFineTuningScreenCSVId
                        orientation: "vertical"
                        size_hint_y: .2
                        pos_hint : {'y':.8}
                        size_hint_x: .5

                        Button:
                                text: 'Menu'
                                on_release: root.manager.current = 'mainScreenName'

                        TextInput:
                                id: text_input
                     ##           size_hint_y: .01
                                height: 30
                                multiline: False
                                on_text_validate: root.textInputActivate()
                                                ## disabled: not checkbox_HU.active

                        Button:
                        ##        size_hint_y: .01
                                on_release: root.show_load()
                                text: 'Selecione um .DATA'


        BoxLayout:

                orientation: "vertical"
                size_hint_y: .08
                pos_hint : {'y':.5}

                TextInput:
                        multiline: False
                        id:labelFineTuningScreenLabelId

